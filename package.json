{
  "name": "langfuse",
  "version": "0.0.25",
  "description": "",
  "main": "dist/module.js",
  "module": "dist/es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "pretty": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "genversion": "genversion -p package.json --es6 --semi ./src/lib/version.ts",
    "prebuild": "npm i && npm run generateAPI && npm run genversion && npm run pretty",
    "build": "rm -rf dist && rollup -c --bundleConfigAsCjs",
    "prepublishOnly": "npm run build",
    "generateAPI": "openapi-typescript src/api/openapi-server.yaml --output src/api/server.ts && openapi-typescript src/api/openapi-client.yaml --output src/api/client.ts && npm run pretty"
  },
  "keywords": [],
  "author": "langfuse",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "genversion": "^3.1.1",
    "openapi-typescript": "^6.2.8",
    "prettier": "^2.8.8",
    "rollup": "^3.26.2",
    "rollup-plugin-typescript2": "^0.35.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "openapi-fetch": "^0.6.0",
    "uuid": "^9.0.0"
  }
}
